{
  "extends": "airbnb-base",
  "env": {
    "browser": true,
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  },
  "rules": {
    "semi": 2,
    "no-param-reassign": 0,
    "no-unreachable": 0,
    "no-undef": 0,
    "no-unused-expressions": 0,
    "dot-notation": 0,
    "prefer-template": 0
  }
}